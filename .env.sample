# Client ID for your Zoom App
ZM_CLIENT_ID=

# Client Secret for your Zoom app
ZM_CLIENT_SECRET=

# Redirect URI set for your app in the Zoom Marketplace
ZM_REDIRECT_URL=

# App Name used for isolating logs
APP_NAME=${_APP_NAME}

# Key used Sign Session Cookies
SESSION_SECRET=${_SESSION_SECRET}

# Zoom Webhook Secret Token (for RTMS webhook validation)
ZM_SECRET_TOKEN=

# Elasticsearch
ELASTICSEARCH_URL=http://localhost:9200

# Jina AI (for semantic search embeddings)
JINA_API_KEY=

# Anthropic (for chat completions)
ANTHROPIC_API_KEY=
